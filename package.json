    "remix-auth": "^3.7.0",
    "remix-auth-form": "^1.5.0",
    "remix-themes": "^1.5.0",
    "remix-utils": "^7.6.0",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7",
